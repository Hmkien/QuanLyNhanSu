    ViewData["Title"] = "Delete";
}

<h3>Xóa chức vụ</h3>

<h3>Bạn có chắc chắn muốn xóa chức vụ này?</h3>
<div>

    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.TenChucVu)
